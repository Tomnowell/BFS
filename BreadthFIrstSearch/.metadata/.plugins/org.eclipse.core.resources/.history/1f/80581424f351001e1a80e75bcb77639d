import java.util.LinkedList;
import java.util.Queue;

/**
 * 
 */

/**
 * @author tom
 *
 */
public class Frontier {
	
	// Establish a FIFO queue
	Queue<Node> frontier = new LinkedList<Node>();
	
	
	public boolean isEmpty() {
		return frontier.peek() == null
		
	}
	
	public Node pop() {
		// Return the top of the queue AND remove
		return frontier.poll();
	}
	
	public Node top() {
		// Return the top of the queue but do not remove
		return frontier.element();
	}
	
	public boolean add(Node node) {
		// Add a node to the start of the queue */
		return frontier.add(node);
	}
	
	
	
}
