import java.util.ArrayList;

/**
 * 
 */

/**
 * @author tom
 *
 */
public class Problem {
	State initialState = new State(true, true, 'L');
		
	ArrayList<State> stateSpace = new ArrayList<State>();

	this.stateSpace.add(initialState);
	
	public void addState(State state) {
		this.stateSpace.add(state);
	}
	
	// Define goal state
	static boolean isGoalState(State state) {
		if (state.dirtLeft == false && state.dirtRight == false) {
			return true;
		}
		return false;
	}
	

}
