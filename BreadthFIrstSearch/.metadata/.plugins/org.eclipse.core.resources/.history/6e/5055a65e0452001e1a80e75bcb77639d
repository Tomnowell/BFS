import java.util.Stack;

/**
 * 
 */

/**
 * @author tom
 *
 */
public class BreadthFirstSearch {

	/**
	 * @param args
	 */
	Problem problem = new Problem();
	
	public Node BreadthFirstSearch(Problem problem) {
		this.problem = problem
	}
	Node startNode = new Node(null, this.problem.initialState, 1);
	
	Stack<State> reached = new Stack<State>();
	
	public void search () {
		
		
		this.reached.push(this.problem.initialState);
		
		if (problem.isGoalState(startNode.state) {
			return reached.peak().
		}
	}
	
	

}
